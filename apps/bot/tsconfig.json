{
  "compilerOptions": {
      "target": "ES6",
      "rootDir": "./src",
      "module": "commonjs",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "strictNullChecks": false,
      "esModuleInterop": true,
      "pretty": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "importHelpers": true,
      "resolveJsonModule": true,
      "lib": ["es2021"]
  },
  "include": ["./src/**/*", "dist/prv/gitUpdate.ts"]
}